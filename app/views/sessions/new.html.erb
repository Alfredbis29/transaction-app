<%= render 'shared/header', text:"LOGIN", path:root_path %>
<div class="container">
<div class="form-container">
<h1>LOGIN</h1>
<%= form_with(url:login_url,method: "post",local:true)do |f| %>
<fieldset>
<%= f.label :name, 'Name' %>
  <%= f.text_field :name %>
</fieldset>
  <%= f.submit "Login",class:'button bg-blue' %>
<% end %>
</div>
</div>